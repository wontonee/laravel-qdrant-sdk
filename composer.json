{
  "name": "wontonee/laravel-qdrant-sdk",
  "description": "Laravel SDK for Qdrant vector database with embedders like OpenAI and Gemini",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wontonee",
      "email": "saju@wontonee.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wontonee\\LarQ\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wontonee\\LarQ\\Providers\\LarQServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}